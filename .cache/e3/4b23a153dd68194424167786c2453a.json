itio web de Stephen King de Joan Melsión */\r\n\r\n@import url('https://fonts.googleapis.com/css2?family=Inconsolata&display=swap');\r\n\r\nbody {\r\n    font-family: 'Inconsolata', monospace;\r\n    color: #000;\r\n}\r\n\r\nh1,\r\nh1,\r\nh2 {\r\n    line-height: 1em;\r\n    margin-bottom: 1.5rem;\r\n}\r\n\r\n.h1 {\r\n    /* font-family: Helvetica, Arial, sans-serif; */\r\n    font-size: 2rem;\r\n    text-align: center;\r\n    font-weight: bold;\r\n    -webkit-animation: bounceIn 3s;\r\n    -webkit-animation-direction: alternate-reverse;\r\n    -webkit-animation-timing-function: ease-in-out;\r\n    -webkit-animation-iteration-count: 5;\r\n}\r\n\r\n.h1:hover {\r\n    -webkit-animation-play-state: paused;\r\n  }\r\n\r\n.h1>span:first-child {\r\n    font-weight: 400;\r\n}\r\n\r\nh1 {\r\n    font-size: 2.5rem;\r\n}\r\n\r\nh2 {\r\n    font-size: 2rem;\r\n}\r\n\r\np {\r\n    font-size: 1.1rem;\r\n    margin-bottom: 1rem;\r\n}\r\n\r\nabbr {\r\n    text-decoration: none;\r\n    cursor: help;\r\n}\r\n\r\na {\r\n    color: inherit;\r\n}\r\n\r\na:hover {\r\n    color: rgba(172, 28, 28, 0.911);\r\n    text-decoration: none;\r\n}\r\n\r\na.pelicula {\r\n    font-size: 0.75rem;\r\n    color: #000;\r\n    margin-top: 50px;\r\n    padding-top: 15px;\r\n    height: 60px;\r\n    text-align: center;\r\n    /* border: 1px solid blue; */\r\n}\r\n\r\na.volver {\r\n    font-size: 1.1rem;\r\n    color: rgba(172, 28, 28, 0.911);\r\n    margin-top: 50px;\r\n    padding-top: 15px;\r\n    height: 60px;\r\n    text-align: center;\r\n    /* border: 1px solid blue; */\r\n}\r\n\r\n/*Clase para esconder un elemento que será visible únicamente a los lectores de pantalla (aplicado para mejorar la accesibilidad: enlace que permite ir directamente al contenido principal de la página y acceso a la visualizació directa del video en Youtube)*/\r\n.sr {\r\n    position: absolute;\r\n    height: 1px;\r\n    width: 1px;\r\n    overflow: hidden;\r\n    clip: rect(1px, 1px, 1px, 1px);\r\n}\r\n\r\n/*Clases aplicadas a las imágenes y vídeos del sitio */\r\n\r\nimg {\r\n    max-width: 100%;\r\n    height: auto;\r\n}\r\n\r\n/* img.izquierda {\r\n    float: left;\r\n  }\r\n  img.derecha {\r\n    float: right;\r\n  } */\r\n\r\nfigure {\r\n    width: 75%;\r\n    margin: 30px auto;\r\n    text-align: center;\r\n}\r\n\r\nfigcaption {\r\n    padding-top: 15px;\r\n    padding-bottom: 15px;\r\n    font-size: 1rem;\r\n}\r\n\r\n.video-fluid {\r\n    position: relative;\r\n    display: block;\r\n    width: 100%;\r\n    padding: 0;\r\n    overflow: hidden;\r\n}\r\n\r\n.video-fluid-item {\r\n    position: absolute;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    border: 0;\r\n}\r\n\r\n.video-fluid::before {\r\n    display: block;\r\n    content: \"\";\r\n    padding-top: 56.25%;\r\n}\r\n\r\n/*Contenedores del sitio */\r\n\r\nheader.header {\r\n    padding-top: 15px;\r\n    padding-bottom: 15px;\r\n    text-align: center;\r\n    /* border: 1px solid blue; */\r\n}\r\n\r\nfooter.footer {\r\n    margin-top: 50px;\r\n    padding-top: 15px;\r\n    height: 60px;\r\n    /* font-family: Helvetica, Arial, sans-serif; */\r\n    text-align: center;\r\n    border-top: 1px solid #000;\r\n    /* border: 1px solid blue; */\r\n}\r\n\r\nfooter.footer p {\r\n    font-size: 0.8rem;\r\n}\r\n\r\n.container {\r\n    width: 90%;\r\n    max-width: 1140px;\r\n    margin: 50px auto;\r\n    /* border: 1px solid red; */\r\n}\r\n\r\n/*Menú de navegación */\r\n\r\nnav {\r\n    /* font-family: Helvetica, Arial, sans-serif; */\r\n    font-size: 1.3rem;\r\n    border-bottom: 1px solid #000;\r\n}\r\n\r\nnav ul {\r\n    list-style: none;\r\n    text-transform: uppercase;\r\n    font-size: 0.9rem;\r\n    text-align: center;\r\n    padding: 0;\r\n    margin: 0;\r\n}\r\n\r\nnav ul li {\r\n    padding: 3px;\r\n}\r\n\r\nnav ul li a {\r\n    color: #000;\r\n    text-decoration: none;\r\n}\r\n\r\nnav ul li.active a {\r\n    font-weight: 900;\r\n}\r\n\r\n/*Estilos para los artículos */\r\n\r\n.summary {\r\n    text-align: center;\r\n    font-size: 1.75rem;\r\n}\r\n\r\n.metadata ul {\r\n    list-style-type: none;\r\n    text-align: center;\r\n    font-size: 1.3rem;\r\n    padding-bottom: 1rem;\r\n}\r\n\r\nfigure.cite {\r\n    border-left: 5px solid #000;\r\n    padding-left: 15px;\r\n    text-align: left;\r\n}\r\n\r\nfigure.cite blockquote {\r\n    margin-left: 0;\r\n}\r\n\r\nfigure.cite figcaption {\r\n    text-align: left;\r\n    font-size: 1.1rem;\r\n}\r\n\r\narticle header {\r\n    text-align: center;\r\n}\r\n\r\narticle footer {\r\n    text-align: center;\r\n    margin-top: 50px;\r\n}\r\n\r\narticle footer p {\r\n    font-size: 0.75rem;\r\n}\r\n\r\narticle ol li {\r\n    font-size: 1.3rem;\r\n}\r\n\r\narticle ol ul li {\r\n    font-size: 1.1rem;\r\n}\r\n\r\ndt {\r\n    font-size: 1.5rem;\r\n    margin-top: 3rem;\r\n}\r\n\r\ndt cite {\r\n    font-style: normal;\r\n}\r\n\r\nselect {\r\n    margin-top: 0px;\r\n    padding-top: 15px;\r\n    padding: .25em;\r\n    border: 0;\r\n    border-bottom: 2px solid currentcolor; \r\n    letter-spacing: .15em;\r\n    border-radius: 0;\r\n  }\r\n\r\n   footer img {\r\n    width: 220px;\r\n    height: 270px;\r\n    margin: 2px;\r\n  }\r\n\r\n  div.lista_imagen {\r\n    width: 35%;\r\n    margin-left: 0px auto;\r\n    padding: 0px;\r\n  }\r\n\r\n  div.lista_imagen a {\r\n    margin-left: 10px;\r\n  }\r\n\r\n  div.lista_imagen img.derecha{\r\n    float: left;\r\n    margin-right: 1px;\r\n  }\r\n\r\n  @-webkit-keyframes bounceIn{\r\n    0% { \r\n      transform: scale(0.1); \r\n      -webkit-transform: scale(0.1);\r\n      opacity: 0;\r\n    } \r\n  \r\n    60% { \r\n      transform: scale(1.2); \r\n      -webkit-transform: scale(1.2);\r\n      opacity: 1;\r\n    } \r\n  \r\n    100% { \r\n      transform: scale(1); \r\n      -webkit-transform: scale(1);\r\n    }\r\n  }\r\n  \r\n  @keyframes bounceIn{\r\n    0% { \r\n      transform: scale(0.1); \r\n      -webkit-transform: scale(0.1);\r\n      opacity: 0;\r\n    } \r\n  \r\n    60% { \r\n      transform: scale(1.2); \r\n      -webkit-transform: scale(1.2);\r\n      opacity: 1;\r\n    } \r\n  \r\n    100% { \r\n      transform: scale(1); \r\n      -webkit-transform: scale(1);\r\n    }\r\n  }\r\n\r\n  ul.separada {\r\n    line-height: 6;\r\n  }\r\n  \r\n  .clip-svg {\r\n    clip-path: polygon(19% 21%, 52% 8%, 81% 28%, 22% 50%, 56% 83%, 84% 53%);\r\n  }\r\n  \r\n\r\n  /* fin de zona común a todas las resoluciones */\r\n  @media screen and (min-width:800px) {\r\n    body {\r\n      font-size:100%;\r\n      /* ampliamos los textos si mide más de 800px */\r\n    }\r\n  }\r\n  \r\n  /* fin de la zona para más de 800px de ancho de pantalla */\r\n  @media screen and (min-width:1200px) {\r\n    body {\r\n      font-size:120%;\r\n      /* ampliamos más aún los textos si mide más de 1200px */\r\n    }\r\n  }\r\n  \r\n  /* fin de la zona para más de 1200px de ancho de pantalla */"},"lineCount":null}},"error":null,"hash":"2c888cc834d7da31f4658b3d4b328466","cacheData":{"env":{}}}